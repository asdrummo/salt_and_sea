<h1>Listing processed_locations</h1>

<table>
  <tr>
    <th>Location</th>
    <th>Date</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @processed_locations.each do |processed_location| %>
  <tr>
    <td><%= processed_location.location_id %></td>
    <td><%= processed_location.date %></td>
    <td><%= link_to 'Show', processed_location %></td>
    <td><%= link_to 'Edit', edit_processed_location_path(processed_location) %></td>
    <td><%= link_to 'Destroy', processed_location, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Processed location', new_processed_location_path %>
